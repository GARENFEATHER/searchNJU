<% @courses.each do |course| %>
<tr>
	<td><%= course.id %></td>
	<td><%= course.name %></td>
	<td><%= course.point %></td>
	<td><%= course.time %></td>
	<td><%= course.area %></td>
	<td><%= course.teacher %></td>
	<td><%= course.academy %></td>
	<td><%= course.special %></td>
	<td><%= course.term %></td>
</tr>
<% end %>